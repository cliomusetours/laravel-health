{
    "name": "cliomusetours/laravel-health",
    "description": "Minimal Laravel health-check package providing liveness and readiness endpoints",
    "keywords": [
        "laravel",
        "health-check",
        "monitoring",
        "liveness",
        "readiness"
    ],
    "homepage": "https://github.com/cliomusetours/laravel-health",
    "license": "MIT",
    "authors": [
        {
            "name": "Giorgos Kalmoukis",
            "email": "giorgoskalmoukis@theloom.gr",
            "homepage": "https://github.com/gkalmoukis",
            "role": "SWE"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "illuminate/console": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Cliomusetours\\LaravelHealth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cliomusetours\\LaravelHealth\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cliomusetours\\LaravelHealth\\HealthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
